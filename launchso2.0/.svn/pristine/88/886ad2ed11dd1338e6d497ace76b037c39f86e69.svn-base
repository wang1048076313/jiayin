<script>
    function toTop() {
        var speed = 500;//滑动的速度
        $('body,html').animate({scrollTop: 0}, speed);
        return false;
    }
    $(function () {
        $(window).on('scroll',function () {
            if($(window).scrollTop()<$(window).height()){
                $('.topball').hide()
            }else{
                $('.topball').show()
            }
        });

        if ($(window).width() > 1100) {
            $(".sidebar").show();
        } else {
            $(".sidebar").hide();
        }

    })

</script>
